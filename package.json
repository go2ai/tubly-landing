{
  "name": "go2ai-site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sst bind vite dev --profile=go2ai-dev --stage=dev --host=172.24.177.100 --port=3333",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "deploy:prod": "sst deploy --profile=go2ai-prod --stage=prod",
    "deploy:dev": "sst deploy --profile=go2ai-dev --stage=dev",
    "remove:dev": "sst remove --profile=go2ai-dev --stage=dev",
    "remove:prod": "sst remove --profile=go2ai-prod --stage=prod",
    "sst:dev": "sst dev",
    "test:unit": "vitest",
    "sso": "aws sso login --sso-session=go2ai"
  },
  "devDependencies": {
    "@notionhq/client": "^2.2.13",
    "@playwright/test": "^1.40.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.27.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.16",
    "aws-cdk-lib": "^2.95.1",
    "constructs": "^10.2.69",
    "daisyui": "^3.9.4",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.35.0",
    "postcss": "^8.4.31",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sst": "^2.36.1",
    "svelte": "^4.2.5",
    "svelte-check": "^3.6.0",
    "svelte-kit-sst": "^2.36.1",
    "svelte-preprocess": "^5.1.0",
    "svelte-typewriter": "^3.2.2",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vanilla-lazyload": "^17.8.5",
    "vite": "^4.5.0",
    "vite-imagetools": "^5.1.2",
    "vitest": "^0.32.4"
  },
  "type": "module",
  "dependencies": {
    "svelte-i18n": "^3.7.3"
  }
}
